{
  "name": "ngx-loadable",
  "version": "2.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project demo -o",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs": "ng build --prod --project demo --base-href '/ngx-loadable/'",
    "build:lib": "ng build loadable",
    "npm_pack": "cd dist/loadable && npm pack",
